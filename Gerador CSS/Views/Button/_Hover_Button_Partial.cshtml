        <td><span>px</span></td>
    </tr>
    <tr>
        <td>Raio de Borrão:</td>
        <td> <input id="range_font_shadow_blur_radius_hover" name="Hover_Text_Shadow_Blur_Radius" type="range" class="range" value="@Model.Hover_Text_Shadow_Blur_Radius.Replace("px", string.Empty)" min="0" max="100" onchange="FontShadowBlurRadiusHover(this)" /> </td>
