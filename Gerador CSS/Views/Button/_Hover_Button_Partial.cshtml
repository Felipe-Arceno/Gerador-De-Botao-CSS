        <td> <input id="text_shadow_color_selector_hover" name="Hover_Text_Shadow_Color" style="width: 100px; height: 30px;" onchange="FontShadowColorSelectorHover(this)" value="@Model.Hover_Text_Shadow_Color" type="color" /> </td>
