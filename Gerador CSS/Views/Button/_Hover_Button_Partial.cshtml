@model Gerador_CSS.Models.ButtonModel
@using Gerador_CSS.Models;


<div id="div_hover_options" align="center" class="primary_menu" onclick="OpenHoverButtonOptions()">
    <span>Mouse Sobre o Botão &#9652;</span>
</div>

<table id="hover_options" style="display: none; width: 88%">

    <tbody>
        <tr>
            <td style="width: 160px">Cor de Fundo:</td>
            <td> <input id="background_color_selector_hover" name="Hover_Background_Color" style="width: 100px; height: 30px;" onchange="BackgroundColorSelectorHover(this)" type="color" value="@Model.Hover_Background_Color" /> </td>
        </tr>
    </tbody>
</table>



<div id="div_hover_font_options" align="center" class="secondary_menu" onclick="OpenHoverFontButtonOptions()">
    <span>Fonte &#9652;</span>
</div>
<table id="hover_font_options" style="display: none; width: 88%">
    <tr>
        <td>Cor:</td>
        <td> <input id="color_selector_hover" name="Hover_Color" style="width: 100px; height: 30px;" onchange="ColorSelectorHover(this)" type="color" value="@Model.Hover_Color" /> </td>
    </tr>
    <tr>
        <td>Tamanho:</td>
        <td> <input id="font_size_hover_range" name="Hover_Font_Size" type="range" class="range" value="@Model.Hover_Font_Size.Replace("px", string.Empty)" min="1" max="100" onchange="FontSizeHover(this)" /> </td>
        <td><input id="font_size_number_hover" onchange="FontSizeHoverNumberChanged(this)" min="0" max="100" value="@Model.Hover_Font_Size.Replace("px", string.Empty)" class="input-number" type="number" /></td>
        <td><span>px</span></td>
    </tr>
    <tr>
        <td>Negrito:</td>
        <td> <input id="font_weight_hover_range" name="Hover_Font_Weight" type="range" class="range" value="@Model.Hover_Font_Weight" min="0" max="1000" step="100" onchange="FontWeightHover(this)" /> </td>
        <td><input id="hover_font_weight_number" onchange="FontWeightHoverNumberChanged(this)" step="100" min="0" max="1000" value="@Model.Hover_Font_Weight" class="input-number" type="number" /></td>
    </tr>
    <tr>
        <td>Estilo:</td>
        <td>
            <input type="hidden" id="select_font_style_hover_hidden" value="@Model.Hover_Font_Style" />
            <select class="select" id="select_font_style_hover" name="Hover_Font_Style" onchange="FontStyleHover(this)">
                <option value="normal">Normal</option>
                <option value="italic">Itálico</option>
                <option value="oblique">Oblíquo</option>
                <option value="initial">Inícial</option>
                <option value="inherit">Herdar</option>
            </select>
        </td>
    </tr>
    <tr>
        <td>SOMBRAS</td>
    </tr>
    <tr>
        <td>Habilitar Sombras:</td>
        <td>
            @if (Model.Hover_Text_Shadow_Enable == true)
            {
                <input id="checkbox_font_shadow_enable_hover" type="checkbox" onchange="FontShadowEnableHover(this)" checked />
            }
            else
            {
                <input id="checkbox_font_shadow_enable_hover" type="checkbox" onchange="FontShadowEnableHover(this)" />
            }

        </td>
    </tr>

    <tr>
        <td>Deslocamento Horizontal:</td>
        <td> <input id="range_font_shadow_offset_x_hover" name="Hover_Text_Shadow_Offset_X" type="range" class="range" value="@Model.Hover_Text_Shadow_Offset_X.Replace("px", string.Empty)" min="0" max="100" onchange="FontShadowOffsetXHover(this)" /> </td>
        <td><input id="font_shadow_offset_x_hover_number" onchange="FontShadowOffsetXHoverNumberChanged(this)" min="0" max="100" value="@Model.Hover_Text_Shadow_Offset_X.Replace("px", string.Empty)" class="input-number" type="number" /></td>
        <td><span>px</span></td>
    </tr>
    <tr>
        <td>Deslocamento Vertical:</td>
        <td> <input id="range_font_shadow_offset_y_hover" name="Hover_Text_Shadow_Offset_Y" type="range" class="range" value="@Model.Hover_Text_Shadow_Offset_Y.Replace("px", string.Empty)" min="0" max="100" onchange="FontShadowOffsetYHover(this)" /> </td>
